<div class='row'>
	<%= render(:partial => 'header') %>
		<h2>Edit Paypal</h2>
		<div class='col-sm-2 col-sm-offset-10'>
			<%= link_to "Back to Paypal list", :action => 'paypal', :html => {:class => 'form-control'} %>
		</div>
		<br />
		<%= form_for :paypal, :url => {:action => 'update_paypal', :id => @paypal.id}, :html => {:class => 'delete_paypal', :role => 'form'} do |f| %>
			<%= render :partial => 'paypal_form', :locals => {:f => f, :paypal => @paypal} %>
			<%= f.submit "Update Paypal", :class => "form-control" %>
		<% end %>
	</div>
</div>